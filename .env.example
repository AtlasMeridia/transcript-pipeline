# ElevenLabs Scribe (default transcription engine)
ELEVENLABS_API_KEY=your_elevenlabs_key_here
TRANSCRIPTION_ENGINE=scribe
SCRIBE_MODEL_ID=scribe_v2

# Whisper fallback model size (tiny, base, small, medium, large)
WHISPER_MODEL=base

# Use Claude (recommended)
ANTHROPIC_API_KEY=your_anthropic_key_here

# OR use GPT
OPENAI_API_KEY=your_openai_key_here

# Default LLM for extraction (claude or gpt)
DEFAULT_LLM=claude

# Optional overrides for specific model versions
CLAUDE_MODEL_ID=claude-sonnet-4-5
OPENAI_MODEL_ID=gpt-4o-mini

# Output directory
OUTPUT_DIR=./output

# CORS origins for web interface (comma-separated, or * for all)
# Production: Set to specific domains, e.g., "https://yourdomain.com,https://app.yourdomain.com"
# Development: Use "*" to allow all origins
CORS_ORIGINS=*
